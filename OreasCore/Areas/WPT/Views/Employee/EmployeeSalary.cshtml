<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'EmployeeSalaryCtlr','EmployeeIndexCtlr', this);">Employee</a>
    <MyViewName child="true"> Salary</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Employee Name
                    </th>
                    <th>
                        CNIC
                    </th>
                    <th>
                        Department / Designation
                    </th>
                </tr>
                <tr>
                    <td>
                        <b>{{MasterObject.EmployeeName}}</b> [{{MasterObject.ATEnrollmentNo_Default}}]
                    </td>
                    <td>
                        {{MasterObject.CNIC}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_WPT_DepartmentDetail_Section_IDName}} / {{MasterObject.FK_tbl_WPT_Designation_IDName}}
                    </td>
                </tr>
                <tr>
                    <td colspan="3">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_WPT_EmployeeSalaryStructure.ID", "{{tbl_WPT_EmployeeSalaryStructure.ID}}")
            @Html.Hidden("tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_Employee_ID", "{{tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_Employee_ID}}")
            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Effective Date</span>
                        <input id="tbl_WPT_EmployeeSalaryStructure.EffectiveDate" name="tbl_WPT_EmployeeSalaryStructure.EffectiveDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_WPT_EmployeeSalaryStructure.EffectiveDate}}" ng-model="tbl_WPT_EmployeeSalaryStructure.EffectiveDate" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.EffectiveDate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Basic Wage</span>
                        <input id="tbl_WPT_EmployeeSalaryStructure.BasicWage" name="tbl_WPT_EmployeeSalaryStructure.BasicWage" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_WPT_EmployeeSalaryStructure.BasicWage" ng-disabled="ng_readOnly" min="1" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.BasicWage" required="true" minlength="1"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">OT Policy</span>
                        <select id="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_tbl_OTPolicy_ID" name="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_tbl_OTPolicy_ID" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.PolicyName for x in OTPolicyList" data-ng-model="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_tbl_OTPolicy_ID" ng-disabled="ng_readOnly" required>
                            <option value="">Select OT Policy</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_tbl_OTPolicy_ID" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input id="tbl_WPT_EmployeeSalaryStructure.Remarks" name="tbl_WPT_EmployeeSalaryStructure.Remarks" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="...." value="tbl_WPT_EmployeeSalaryStructure.Remarks" ng-model="tbl_WPT_EmployeeSalaryStructure.Remarks" ng-disabled="ng_readOnly" minlength="3" maxlength="50" />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.Remarks" minlength="3" maxlength="50"></MyValidationSpan>
                    </div>
                   
                </div>
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Primary Transaction Mode</span>
                        <select id="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID" name="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.TransactionMode for x in TransactionModeList" data-ng-model="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID" ng-disabled="ng_readOnly" required>
                            <option value="">Select Transaction Mode</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Max Transaction Limit</span>
                        <input id="tbl_WPT_EmployeeSalaryStructure.MaxTransactionLimit" name="tbl_WPT_EmployeeSalaryStructure.MaxTransactionLimit" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_WPT_EmployeeSalaryStructure.MaxTransactionLimit" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.MaxTransactionLimit" required="true" minlength="1"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Secondary Transaction Mode</span>
                        <select id="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID_Secondary" name="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID_Secondary" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.TransactionMode for x in TransactionModeList" data-ng-model="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID_Secondary" ng-disabled="ng_readOnly">
                            <option value="">Select Transaction Mode</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_EmployeeSalaryStructure.FK_tbl_WPT_TransactionMode_ID_Secondary"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
          
                    </div>
                </div>


                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_EmployeeSalaryStructure.CreatedBy}}" ng-model="tbl_WPT_EmployeeSalaryStructure.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_EmployeeSalaryStructure.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_EmployeeSalaryStructure.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_EmployeeSalaryStructure.ModifiedBy}}" ng-model="tbl_WPT_EmployeeSalaryStructure.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_EmployeeSalaryStructure.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_EmployeeSalaryStructure.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>
    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    EffectiveDate
                </th>
                <th>
                    BasicWage
                </th>
                <th>
                    OT Policy
                </th>
                <th>
                    Primary Transaction
                </th>
                <th>
                    Max Transaction Limit
                </th>
                <th>
                    Secondary Transaction
                </th>
                <th>
                    Total Allowances
                </th>
                <th>
                    Total Deductibles
                </th>
                <th>
                    Increment Doc#
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.EffectiveDate}}
                </td>
                <td>
                    {{itm.BasicWage}}
                </td>
                <td>
                    {{itm.FK_tbl_WPT_tbl_OTPolicy_IDName}}
                </td>
                <td>
                    {{itm.FK_tbl_WPT_TransactionMode_IDName}}
                </td>
                <td>
                    {{itm.MaxTransactionLimit}}
                </td>
                <td>
                    {{itm.FK_tbl_WPT_TransactionMode_ID_SecondaryName}}
                </td>
                <td>
                    {{itm.TotalAllowances}}
                </td>
                <td>
                    {{itm.TotalDeductibles}}
                </td>
                <td>
                    {{itm.FK_tbl_WPT_IncrementDetail_IDName}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('EmployeeSalaryAllowanceCtlr',itm,'EmployeeSalaryCtlr','EmployeeSalaryAllowanceCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Allowance Detail">Allowance <span class="fa fa-arrow-circle-up"></span></button>|
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('EmployeeSalaryDeductibleCtlr',itm,'EmployeeSalaryCtlr','EmployeeSalaryDeductibleCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Deductible Detail">Deductible <span class="fa fa-arrow-circle-down"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="10">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "EmployeeSalaryCtlr")

</div>
