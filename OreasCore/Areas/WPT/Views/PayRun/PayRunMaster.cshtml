<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'PayRunMasterCtlr','PayRunIndexCtlr', this);">PayRun</a>
    <MyViewName child="true"> Period</MyViewName>
</MyViewName>

<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Month From
                    </th>
                    <th>
                        Month To
                    </th>
                </tr>
                <tr>
                    <td>
                        <b>{{MasterObject.CalendarMonthStartDate}}</b>
                    </td>
                    <td>
                        <b>{{MasterObject.CalendarMonthEndDate}}</b>
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_WPT_PayRunMaster.ID", "{{tbl_WPT_PayRunMaster.ID}}")
            @Html.Hidden("tbl_WPT_PayRunMaster.FK_tbl_WPT_CalendarYear_Months_ID", "{{tbl_WPT_PayRunMaster.FK_tbl_WPT_CalendarYear_Months_ID}}")

            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Date From</span>
                        <input id="tbl_WPT_PayRunMaster.StartDate" name="tbl_WPT_PayRunMaster.StartDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_WPT_PayRunMaster.StartDate}}" min="{{MinRange|date:'yyyy-MM-dd'}}" max="{{MaxRange|date:'yyyy-MM-dd'}}" ng-model="tbl_WPT_PayRunMaster.StartDate" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_PayRunMaster.StartDate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Date Till</span>
                        <input id="tbl_WPT_PayRunMaster.EndDate" name="tbl_WPT_PayRunMaster.EndDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_WPT_PayRunMaster.EndDate}}" min="{{MinRange|date:'yyyy-MM-dd'}}" max="{{MaxRange|date:'yyyy-MM-dd'}}" ng-model="tbl_WPT_PayRunMaster.EndDate" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_PayRunMaster.EndDate" required="true"></MyValidationSpan>
                    </div>
                </div>                

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_PayRunMaster.CreatedBy}}" ng-model="tbl_WPT_PayRunMaster.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_PayRunMaster.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_PayRunMaster.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_PayRunMaster.ModifiedBy}}" ng-model="tbl_WPT_PayRunMaster.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_PayRunMaster.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_PayRunMaster.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>

            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th style="background-color:lightsteelblue;">
                    Start Date
                </th>
                <th style="background-color:lightsteelblue;">
                    End Date
                </th>
                <th>
                    Active Emp
                </th>
                <th style="background-color:lightgreen;">
                    AT Emp
                </th>
                <th style="background-color:lightpink;">
                    0 AT Emp
                </th>
                <th style="background-color:lightpink;">
                    0 Wage Emp
                </th>
                <th style="background-color:orangered;">
                    Defaulters
                </th>
                <th>
                    Total Wage
                </th>
                <th style="background-color:indianred;">
                    &#80; &#8804; &#50; Emp
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.StartDate}}
                </td>
                <td>
                    {{itm.EndDate}}
                </td>
                <td>
                    {{itm.NoOfActiveEmployees}}
                </td>
                <td>
                    {{itm.NoOfEmployees}}
                </td>
                <td>
                    {{itm.NoOfZeroATEmp}}
                </td>
                <td>
                    {{itm.NoOfZeroWageEmp}}
                </td>
                <td>
                    {{itm.NoOfDefaulterWageEmp}}
                </td>
                <td>
                    {{itm.TotalWage}}
                </td>
                <td>
                    {{itm.NoOf2LessThenEqualATEmp}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('PayRunMasterDetailEmployeeCtlr',itm,'PayRunMasterCtlr','PayRunMasterDetailEmployeeCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Employee">Employee<span class="fa fa-user"></span></button>|
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('PayRunMasterDetailPaymentCtlr',itm,'PayRunMasterCtlr','PayRunMasterDetailPaymentCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Payment">Pay<span class="fa fa-money-bill"></span></button>|
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('PayRunOperationHubCtlr',itm,'PayRunMasterCtlr','PayRunOperationHubCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Bulk Operation">Bulk<span class="fa fa-walking"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="10">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
   

</div>
