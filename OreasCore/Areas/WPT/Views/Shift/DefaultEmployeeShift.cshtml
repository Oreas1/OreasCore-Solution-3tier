<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'DefaultEmployeeShiftCtlr','ShiftIndexCtlr', this);">Shift</a>
    <MyViewName child="true"> Default Emloyee Shift</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Shift Name / Prefix
                    </th>
                    <th>
                        Shift Start / End
                    </th>
                    <th>
                        LateIn / EarlyOut
                    </th>
                    <th>
                        CheckIn / CheckOut
                    </th>
                </tr>
                <tr>
                    <td>
                        <b><u>{{MasterObject.ShiftName}}</u></b> [{{MasterObject.Prefix}}]
                    </td>
                    <td>
                        {{MasterObject.StartTime}} / {{MasterObject.EndTime}}
                    </td>
                    <td>
                        {{MasterObject.LateInTime}} / {{MasterObject.EarlyOutTime}}
                    </td>
                    <td>
                        {{MasterObject.CheckInStartTime}} / {{MasterObject.CheckOutEndTime}}
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <div class="" style="padding:5px;">
        <div class="row">
            <div class="form-group col-lg-3 col-md-4 col-sm-4">
                <select id="FK_tbl_WPT_DepartmentDetail_Section_ID" name="FK_tbl_WPT_DepartmentDetail_Section_ID" ng-options="x.ID as x.SectionName for x in SectionList" data-ng-model="FK_tbl_WPT_DepartmentDetail_Section_ID" class="form-control form-select" style="color:steelblue; font-weight:bold;" ng-disabled="readOnlyOnDelete">
                    <option value="">Please Select Section</option>
                </select>
            </div>
            <div class="form-group col-lg-3 col-md-4 col-sm-4">
                <select id="FK_tbl_WPT_Designation_ID" name="FK_tbl_WPT_Designation_ID" ng-options="x.ID as x.Designation for x in DesignationList" data-ng-model="FK_tbl_WPT_Designation_ID" class="form-control form-select" style="color:steelblue; font-weight:bold;"  ng-disabled="readOnlyOnDelete">
                    <option value="">Please Select Designation</option>
                </select>
            </div>
            <div class="form-group col-lg-3 col-md-4 col-sm-4">
                <select id="BulkBy" name="BulkBy" ng-options="o.v as o.n for o in BulkByList" data-ng-model="BulkBy" class="form-control form-select" style="color:steelblue; font-weight:bold;"  ng-disabled="readOnlyOnDelete">
                    <option value="">Please Select By</option>
                </select>
            </div>
            <div class="form-group col-lg-2 col-md-2 col-sm-2">
                <input type="button" value="Update Bulk" class="btn btn-primary form-control" ng-click="UpdateBulk()" ng-disabled="!(BulkBy.length>0) || !(FK_tbl_WPT_DepartmentDetail_Section_ID>0 || FK_tbl_WPT_Designation_ID>0)" />
            </div>
        </div>
    </div>
    <br />
    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Employee Name
                </th>
                <th>
                    Employee No
                </th>
                <th>
                    AT No
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.EmployeeName}}
                </td>
                <td>
                    {{itm.EmployeeNo}}
                </td>
                <td>
                    {{itm.ATEnrollmentNo_Default}}
                </td>
                <td>
                </td>
            </tr>
            <tr>
                <td colspan="4">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "DefaultEmployeeShiftCtlr")

</div>
