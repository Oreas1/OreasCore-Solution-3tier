<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'BankDetailBranchCtlr','BanktIndexCtlr', this);">Bank</a>
    <MyViewName child="true"> Branch</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Bank Name
                    </th>
                </tr>
                <tr>
                    <td>
                        <b>{{MasterObject.BankName}}</b>
                    </td>
                </tr>
                <tr>
                    <td colspan="1">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_WPT_Bank_Branch.ID", "{{tbl_WPT_Bank_Branch.ID}}")
            @Html.Hidden("tbl_WPT_Bank_Branch.FK_tbl_WPT_Bank_ID", "{{tbl_WPT_Bank_Branch.FK_tbl_WPT_Bank_ID}}")
            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Branch Name</span>
                        <input id="tbl_WPT_Bank_Branch.BranchName" name="tbl_WPT_Bank_Branch.BranchName" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="BranchName" value="tbl_WPT_Bank_Branch.BranchName" ng-model="tbl_WPT_Bank_Branch.BranchName" ng-disabled="ng_readOnly" minlength="3" maxlength="50" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_Bank_Branch.BranchName" required="true" minlength="3" maxlength="50"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Branch Code</span>
                        <input id="tbl_WPT_Bank_Branch.BranchCode" name="tbl_WPT_Bank_Branch.BranchCode" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="BranchCode" value="tbl_WPT_Bank_Branch.BranchCode" ng-model="tbl_WPT_Bank_Branch.BranchCode" ng-disabled="ng_readOnly" minlength="3" maxlength="50" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_Bank_Branch.BranchCode" required="true" minlength="3" maxlength="50"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">City</span>
                        <input id="tbl_WPT_Bank_Branch.City" name="tbl_WPT_Bank_Branch.City" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="City" value="tbl_WPT_Bank_Branch.City" ng-model="tbl_WPT_Bank_Branch.City" ng-disabled="ng_readOnly" maxlength="50" />
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_Bank_Branch.City" maxlength="50"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Postal Address</span>
                        <input id="tbl_WPT_Bank_Branch.PostalAddress" name="tbl_WPT_Bank_Branch.PostalAddress" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="PostalAddress" value="tbl_WPT_Bank_Branch.PostalAddress" ng-model="tbl_WPT_Bank_Branch.PostalAddress" ng-disabled="ng_readOnly" maxlength="100"/>
                        <MyValidationSpan formname="myform" fieldname="tbl_WPT_Bank_Branch.PostalAddress" maxlength="100"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
            
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">

                    </div>
                </div>


                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_Bank_Branch.CreatedBy}}" ng-model="tbl_WPT_Bank_Branch.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_Bank_Branch.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_Bank_Branch.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_Bank_Branch.ModifiedBy}}" ng-model="tbl_WPT_Bank_Branch.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_WPT_Bank_Branch.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_WPT_Bank_Branch.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>
    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Branch Name
                </th>
                <th>
                    Branch Code
                </th>
                <th>
                    City
                </th>
                <th>
                    Postal Address
                </th>
                <th>
                    No Of Company A/c
                </th>
                <th>
                    No Of Employees A/c
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.BranchName}}
                </td>
                <td>
                    {{itm.BranchCode}}
                </td>
                <td>
                    {{itm.City}}
                </td>
                <td>
                    {{itm.PostalAddress}}
                </td>
                <td>
                    {{itm.NoOfCompanyAc}}
                </td>
                <td>
                    {{itm.NoOfEmpAc}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('BankDetailBranchCompanyAcCtlr',itm,'BankDetailBranchCtlr','BankDetailBranchCompanyAcCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Company Account">Company A/c <span class="fa fa-building"></span></button>|
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('BankDetailBranchEmployeeAcCtlr',itm,'BankDetailBranchCtlr','BankDetailBranchEmployeeAcCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Employee Account">Employee A/c <span class="fa fa-user"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="7">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "BankDetailBranchCtlr")

</div>
