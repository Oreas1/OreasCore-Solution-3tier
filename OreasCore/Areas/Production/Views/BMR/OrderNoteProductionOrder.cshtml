<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'OrderNoteProductionOrderCtlr','OrderNoteProductionCtlr', this);">Order Note Production</a>
    <MyViewName child="true"> Production Order</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Order #
                    </th>
                    <th>
                        Target Date
                    </th>
                    <th>
                        Priority
                    </th>
                    <th>
                        Primary Product
                    </th>
                    <th>
                        Packaging Name
                    </th>
                    <th>
                        Req Qty
                    </th>
                    <th>
                        Manufacturing Qty
                    </th>
                    <th>
                        Sold Qty
                    </th>
                    <th>
                        Status
                    </th>
                    <th>
                        Customer Name
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.TargetDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_AspNetOreasPriority_IDName}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Pro_CompositionDetail_Coupling_PackagingMaster_IDName}}
                    </td>
                    <td>
                        {{MasterObject.MfgQtyLimit}} {{MasterObject.MeasurementUnit}}
                    </td>
                    <td>
                        {{MasterObject.ManufacturingQty}} {{MasterObject.MeasurementUnit}}
                    </td>
                    <td>
                        {{MasterObject.SoldQty}} {{MasterObject.MeasurementUnit}}
                    </td>
                    <td>
                        {{MasterObject.ClosedTrue_OpenFalse? 'Closed' : 'Open'}}
                    </td>
                    <td>
                        {{MasterObject.CustomerName}}
                    </td>
                </tr>
                <tr>
                    <td colspan="10">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Inv_OrderNoteDetail_ProductionOrder.ID", "{{tbl_Inv_OrderNoteDetail_ProductionOrder.ID}}")
            @Html.Hidden("tbl_Inv_OrderNoteDetail_ProductionOrder.FK_tbl_Inv_OrderNoteDetail_ID", "{{tbl_Inv_OrderNoteDetail_ProductionOrder.FK_tbl_Inv_OrderNoteDetail_ID}}")

            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Req BatchSize In {{MasterObject.MeasurementUnit}}</span>
                        <input id="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchSize" name="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchSize" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchSize" ng-pattern="wholeNumberOrNot" ng-disabled="ng_readOnly" min="1" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchSize" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Req BatchNo</span>
                        <input type="text" id="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchNo" name="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchNo" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="RequestedBatchNo" value="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchNo" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchNo" ng-disabled="ng_readOnly" maxlength="25" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBatchNo" maxlength="25" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Req Mfg Date</span>
                        <input id="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedMfgDate" name="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedMfgDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedMfgDate}}" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedMfgDate" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedMfgDate" required="true"></MyValidationSpan>
                    </div>
                </div>



                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Requested By</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBy}}" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Requested Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.RequestedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Processed By</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrderNoteDetail_ProductionOrder.ProcessedBy}}" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.ProcessedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Processed Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrderNoteDetail_ProductionOrder.ProcessedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_OrderNoteDetail_ProductionOrder.ProcessedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Req Batch Size
                </th>
                <th>
                    Req BatchNo
                </th>
                <th>
                    Req Mfg Date
                </th>
                <th>
                    Req By
                </th>
                <th>
                    Req Date
                </th>
                <th>
                    Processed By
                </th>
                <th>
                    Processed Date
                </th>
                <th>
                    Processed BatchNo
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.RequestedBatchSize}} {{MasterObject.MeasurementUnit}}
                </td>
                <td>
                    {{itm.RequestedBatchNo}}
                </td>
                <td>
                    {{itm.RequestedMfgDate}}
                </td>
                <td>
                    {{itm.RequestedBy}}
                </td>
                <td>
                    {{itm.RequestedDate}}
                </td>
                <td>
                    {{itm.ProcessedBy}}
                </td>
                <td>
                    {{itm.ProcessedDate}}
                </td>
                <td>
                    {{itm.ProcessedBatchNo}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')" ng-hide="itm.ProcessedBatchNo.length > 0"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')" ng-hide="itm.ProcessedBatchNo.length > 0"></MyButton>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="9">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------Availability-----------------------------*@
    <MyFieldSet legendtext="">
        <div mycssclass="@MyCSSClassName.RowInForm">
            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup6">
                <div class="card text-center">
                    <div class="card-header" style="background-color:lightgreen;">
                        <h5>Raw Material <i class="fa fa-leaf" style="color:green;"></i></h5>
                    </div>
                    <div class="card-body">
                        <h6 class="card-title">Available Batch Size: {{MaxRawBatchSizeObj.MaxUnits}}-{{MasterObject.MeasurementUnit}}</h6>
                        <p class="card-text">Due to</p>
                        <p class="card-text"><b>* {{MaxRawBatchSizeObj.ProductName}}</b></p>
                    </div>
                    <div class="card-footer text-muted" style="background-color:lightgreen;">

                    </div>
                </div>
            </div>
            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup6">
                <div class="card text-center">
                    <div class="card-header" style="background-color:lightsteelblue;">
                        <h5>Packaging Material <i class="fa fa-boxes-packing" style="color:steelblue;"></i></h5>
                    </div>
                    <div class="card-body">
                        <h6 class="card-title">Available Batch Size: {{MaxPackagingBatchSizeObj.MaxUnits}}-{{MasterObject.MeasurementUnit}}</h6>
                        <p class="card-text">Due to</p>
                        <p class="card-text"><b>* {{MaxPackagingBatchSizeObj.ProductName}}</b></p>
                    </div>
                    <div class="card-footer text-muted" style="background-color:lightsteelblue;">

                    </div>
                </div>
            </div>
        </div>

        <div mycssclass="@MyCSSClassName.Table_Div">
            <table mycssclass="@MyCSSClassName.Table_Table">
                <tr>
                    <th>
                        Type
                    </th>
                    <th>
                        WareHouse
                    </th>
                    <th>
                        Product
                    </th>
                    <th>
                        Category
                    </th>
                    <td>
                        Max Units
                    </td>
                    <th>
                        Standard Size
                    </th>
                    <th>
                        Formula Qty
                    </th>
                    <th>
                        Available Qty
                    </th>
                </tr>
                <tr ng-repeat="itm in BMRAvailabilityData">
                    <td>
                        {{itm.FilterName}}
                    </td>
                    <td>
                        {{itm.WareHouseName}}
                    </td>
                    <td>
                        {{itm.ProductName}}
                    </td>
                    <td>
                        {{itm.CategoryName}}
                    </td>
                    <td>
                        {{itm.MaxUnits}} {{itm.UnitPrimary}}
                    </td>
                    <td>
                        {{itm.FormulaBatchSize}} {{MasterObject.CouplingUnit}}
                    </td>
                    <td>
                        {{itm.FormulaQty}} {{itm.UnitItem}}
                    </td>
                    <td>
                        {{itm.AvailableQty}} {{itm.UnitItem}}
                    </td>
                </tr>
                <tr>
                    <td colspan="8">
                        <div mycssclass="@MyCSSClassName.Table_NavDiv">
                            <button class="btn btn-primary" ng-click="LoadAvailability();">Get Availability</button>
                        </div>
                    </td>
                </tr>
            </table>
        </div>
    </MyFieldSet>

</div>
