<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'OrdinaryRequisitionDispensingDetailCtlr','OrdinaryRequisitionDispensingMasterCtlr', this);">Ordinary Requisition Dispensing</a>
    <MyViewName child="true"> Detail</MyViewName>
</MyViewName>

<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        WareHouse
                    </th>
                    <th>
                        Section
                    </th>
                </tr>
                <tr>
                    <th>
                        {{MasterObject.DocNo}}
                    </th>
                    <th>
                        {{MasterObject.DocDate}}
                    </th>
                    <th>
                        {{MasterObject.FK_tbl_Inv_WareHouseMaster_IDName}}
                    </th>
                    <th>
                        {{MasterObject.FK_tbl_WPT_DepartmentDetail_Section_IDName}}
                    </th>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>
    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.CancelOfAdd"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Inv_OrdinaryRequisitionDetail.ID", "{{tbl_Inv_OrdinaryRequisitionDetail.ID}}")
            @Html.Hidden("tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionMaster_ID", "{{tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionMaster_ID}}")
            
            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Request For</span>
                        <input type="text" id="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionType_IDName" name="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionType_IDName" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="FK_tbl_Inv_OrdinaryRequisitionType_IDName" value="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionType_IDName" ng-model="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_OrdinaryRequisitionType_IDName" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Required / Return</span>
                        <select id="tbl_Inv_OrdinaryRequisitionDetail.RequiredTrue_ReturnFalse" name="tbl_Inv_OrdinaryRequisitionDetail.RequiredTrue_ReturnFalse" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="o.v as o.n for o in [{ n: 'Required', v: true }, { n: 'Return', v: false }]" data-ng-model="tbl_Inv_OrdinaryRequisitionDetail.RequiredTrue_ReturnFalse" disabled >
                            <option value="">Select Required/Return</option>
                        </select>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Product</span>
                        <input type="text" id="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" name="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="FK_tbl_Inv_ProductRegistrationDetail_IDName" value="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" ng-model="tbl_Inv_OrdinaryRequisitionDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Quantity</span>
                        <input id="tbl_Inv_OrdinaryRequisitionDetail.Quantity" name="tbl_Inv_OrdinaryRequisitionDetail.Quantity" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_OrdinaryRequisitionDetail.Quantity" min="0" disabled />
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm"> 
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Dispensed</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" id="tbl_Inv_OrdinaryRequisitionDetail.IsDispensed" value="{{tbl_Inv_OrdinaryRequisitionDetail.IsDispensed ? 'Yes' : 'No'}}" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input type="text" id="tbl_Inv_OrdinaryRequisitionDetail.Remarks" name="tbl_Inv_OrdinaryRequisitionDetail.Remarks" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Remarks" value="tbl_Inv_OrdinaryRequisitionDetail.Remarks" ng-model="tbl_Inv_OrdinaryRequisitionDetail.Remarks" disabled />
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrdinaryRequisitionDetail.CreatedBy}}" ng-model="tbl_Inv_OrdinaryRequisitionDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrdinaryRequisitionDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_OrdinaryRequisitionDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrdinaryRequisitionDetail.ModifiedBy}}" ng-model="tbl_Inv_OrdinaryRequisitionDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_OrdinaryRequisitionDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_OrdinaryRequisitionDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Requisition Type
                </th>
                <th>
                    Required / Return
                </th>
                <th>
                    Product
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Dispensed
                </th>
                <th>
                    Remarks
                </th>
                <th>
                    Dispensed Qty
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Inv_OrdinaryRequisitionType_IDName}}
                </td>
                <td>
                    {{itm.RequiredTrue_ReturnFalse ? 'Required' : 'Return'}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                </td>
                <td>
                    {{itm.Quantity}} {{itm.MeasurementUnit}}
                </td>
                <td>
                    {{itm.IsDispensed ? 'Yes' : 'No'}}
                    <button mycssclass="@MyCSSClassName.Table_OperationBtn" title="close dispensing without issuance" ng-hide="Privilege.CanAdd === false || itm.IsDispensed === true" ng-click="CloseDispensing(itm.ID);"><span class="fa fa-lock"></span> Close</button>
                </td>
                <td>
                    {{itm.Remarks}}
                </td>
                <td>
                    {{itm.DispensedQty}} {{itm.MeasurementUnit}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>
                        <span ng-hide="!Privilege.CanAdd || itm.Quantity<=itm.DispensedQty || itm.RequiredTrue_ReturnFalse===false">
                            |<button class="btn" ng-click="AutoIssuanceRequest(0,0,0,itm.ID, this);" title="Auto Dispensing FIFO"><i class='fa fa-person-running' style='color: maroon;'></i></button>
                        </span>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="DivHideShow('OrdinaryRequisitionDispensingDetailDispensingCtlr',itm,'OrdinaryRequisitionDispensingDetailCtlr','OrdinaryRequisitionDispensingDetailDispensingCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Dispensing">Dispensing <span class="fa fa-info-circle"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="8">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "OrdinaryRequisitionDispensingDetailCtlr")

</div>
