<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'PDRequestCFPDetailItemCtlr','PDRequestCFPMasterCtlr', this);">PD Request Dispensing</a>
    <MyViewName child="true"> Ingredients</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Product Name
                    </th>
                    <th>
                        Batch No
                    </th>
                    <th>
                        Batch Size
                    </th>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Composition Filter Policy
                    </th>
                    <th>
                        WareHouse
                    </th>
                    <td>
                        Status
                    </td>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.ProductName}}
                    </td>
                    <td>
                        {{MasterObject.TrialBatchNo}}
                    </td>
                    <td>
                        {{MasterObject.TrialBatchSizeInSemiUnits}} {{MasterObject.MeasurementUnit}}
                    </td>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Pro_CompositionFilterPolicyDetail_IDName}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_WareHouseMaster_IDName}}
                    </td>
                    <td>
                        {{itm.TrialStatus===true ? 'Sucessfull' : itm.TrialStatus===false ? 'Failed' : 'Pending'}}
                    </td>
                </tr>
                <tr>
                    <td colspan="8">
                    </td>
                </tr>
            </table>
        </div>
    </div>
    @*----------------------------------- this is entry panel -----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_PD_RequestDetailTR_CFP_Item.ID", "{{tbl_PD_RequestDetailTR_CFP_Item.ID}}")
            @Html.Hidden("tbl_PD_RequestDetailTR_CFP_Item.FK_tbl_PD_RequestDetailTR_CFP_ID", "{{tbl_PD_RequestDetailTR_CFP_Item.FK_tbl_PD_RequestDetailTR_CFP_ID}}")

            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Ingredient Product</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" id="tbl_PD_RequestDetailTR_CFP_Item.FK_tbl_Inv_ProductRegistrationDetail_IDName" value="{{tbl_PD_RequestDetailTR_CFP_Item.FK_tbl_Inv_ProductRegistrationDetail_IDName}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.FK_tbl_Inv_ProductRegistrationDetail_IDName" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Quantity In {{tbl_PD_RequestDetailTR_CFP_Item.MeasurementUnit}}</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" id="tbl_PD_RequestDetailTR_CFP_Item.Quantity" value="{{tbl_PD_RequestDetailTR_CFP_Item.Quantity}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.Quantity" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Required / Return</span>
                        <select id="tbl_PD_RequestDetailTR_CFP_Item.RequiredTrue_ReturnFalse" name="tbl_PD_RequestDetailTR_CFP_Item.RequiredTrue_ReturnFalse" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="o.v as o.n for o in [{ n: 'Required', v: true }, { n: 'Return', v: false }]" data-ng-model="tbl_PD_RequestDetailTR_CFP_Item.RequiredTrue_ReturnFalse" disabled>
                            <option value="">Select Required / Return</option>
                        </select>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Dispensed</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" id="tbl_PD_RequestDetailTR_CFP_Item.IsDispensed" value="{{tbl_PD_RequestDetailTR_CFP_Item.IsDispensed ? 'Yes' : 'No'}}" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" id="tbl_PD_RequestDetailTR_CFP_Item.Remarks" value="{{tbl_PD_RequestDetailTR_CFP_Item.Remarks}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.Remarks" disabled />
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR_CFP_Item.CreatedBy}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR_CFP_Item.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR_CFP_Item.ModifiedBy}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR_CFP_Item.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_PD_RequestDetailTR_CFP_Item.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>

            </MyFieldSet>

        </form>
    </div>

    @*----------------------------------- this is data list view -----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Ingredient Name
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Req / Return
                </th>
                <th>
                    Dispensed
                </th>
                <th>
                    Remarks
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                </td>
                <td>
                    {{itm.Quantity}}
                </td>
                <td>
                    {{itm.RequiredTrue_ReturnFalse ? 'Required' : 'Return'}}
                </td>
                <td>
                    {{itm.IsDispensed === true ? 'Yes' : 'No'}}
                    <button mycssclass="@MyCSSClassName.Table_OperationBtn" title="close dispensing without issuance" ng-hide="Privilege.CanAdd === false || itm.IsDispensed === true" ng-click="CloseDispensing(itm.ID);"><span class="fa fa-lock"></span> Close</button>
                </td>
                <td>
                    {{itm.Remarks}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('PDRequestCFPDetailItemDispensingCtlr',itm,'PDRequestCFPDetailItemCtlr','PDRequestCFPDetailItemDispensingCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Dispensing">Dispensing <span class="fa fa-info-circle"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="6">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "PDRequestCFPDetailItemCtlr")
</div>
