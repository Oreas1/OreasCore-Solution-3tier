<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'SalesReturnNoteDetailCtlr','SalesReturnNoteMasterCtlr', this);">Sales Return Note</a>
    <MyViewName child="true"> Detail</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Ware House
                    </th>
                    <th>
                        Customer A/c
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_WareHouseMaster_IDName}}
                    </td>
                    <td>
                        <b>{{MasterObject.FK_tbl_Ac_ChartOfAccounts_IDName}}</b>
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Inv_SalesReturnNoteDetail.ID", "{{tbl_Inv_SalesReturnNoteDetail.ID}}")
            @Html.Hidden("tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesReturnNoteMaster_ID", "{{tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesReturnNoteMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Product In {{tbl_Inv_SalesReturnNoteDetail.MeasurementUnit}}</span>
                        <MyButtonModal fieldname="tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" ng-click="OpenProductSearchModalGeneral('SalesReturnNote', tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesReturnNoteMaster_ID, 'tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName', ProductSearch_CtrlFunction_Ref_InvokeOnSelection, null, tbl_Inv_SalesReturnNoteDetail.ID); myform['tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName'].$dirty=true;" ng-disabled="ng_readOnly" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Reference #</span>
                        <MyButtonModal fieldname="tbl_Inv_SalesReturnNoteDetail.ReferenceNo" ng-click="OpenReferenceSearchModalGeneral('SalesReturnNote', tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesReturnNoteMaster_ID, 'tbl_Inv_SalesReturnNoteDetail.ReferenceNo', ReferenceSearch_CtrlFunction_Ref_InvokeOnSelection, null, tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_ID); myform['tbl_Inv_SalesReturnNoteDetail.ReferenceNo'].$dirty=true;" ng-disabled="ng_readOnly || !tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_ID > 0" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesReturnNoteDetail.ReferenceNo" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Sales Note #</span>
                        <MyButtonModal fieldname="tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesNoteDetail_IDName" ng-click="OpenSalesNoteSearchModal(); myform['tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesNoteDetail_IDName'].$dirty=true;" ng-disabled="ng_readOnly || !tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_ID > 0 || !tbl_Inv_SalesReturnNoteDetail.ReferenceNo > 0" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesReturnNoteDetail.FK_tbl_Inv_SalesNoteDetail_IDName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Quantity</span>
                        <input id="tbl_Inv_SalesReturnNoteDetail.Quantity" name="tbl_Inv_SalesReturnNoteDetail.Quantity" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesReturnNoteDetail.Quantity" ng-pattern="wholeNumberOrNot" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesReturnNoteDetail.Quantity" required="true"></MyValidationSpan>
                    </div>
                </div>
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input type="text" id="tbl_Inv_SalesReturnNoteDetail.Remarks" name="tbl_Inv_SalesReturnNoteDetail.Remarks" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Remarks" value="tbl_Inv_SalesReturnNoteDetail.Remarks" ng-model="tbl_Inv_SalesReturnNoteDetail.Remarks" ng-disabled="ng_readOnly" maxlength="50" />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesReturnNoteDetail.Remarks" maxlength="50"></MyValidationSpan>
                    </div>
                </div>



                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesReturnNoteDetail.CreatedBy}}" ng-model="tbl_Inv_SalesReturnNoteDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesReturnNoteDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_SalesReturnNoteDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesReturnNoteDetail.ModifiedBy}}" ng-model="tbl_Inv_SalesReturnNoteDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesReturnNoteDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_SalesReturnNoteDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Product Name
                </th>
                <th>
                    Reference #
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Sales #
                </th>
                <th>
                    Remarks
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                </td>
                <td>
                    {{itm.ReferenceNo}}
                </td>
                <td>
                    {{itm.Quantity}} {{itm.MeasurementUnit}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_SalesNoteDetail_IDName}}
                </td>
                <td>
                    {{itm.Remarks}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="6">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "SalesReturnNoteDetailCtlr")
    @await Html.PartialAsync("_ReportPanelGeneral")


    <div class="modal fade" id="SalesNoteSearchModal" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h6 class="modal-title">Select Sales Note</h6>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>
                <div class="modal-body">
                    <form novalidate>
                        <div mycssclass="@MyCSSClassName.RowInForm">
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Filter by</span>
                                <select id="SalesNoteFilterBy" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select"
                                        ng-options="o.v as o.n for o in [{ n: 'Invoice No', v: 'InvoiceNo' },{ n: 'Quantity', v: 'Quantity' }]"
                                        ng-model="SalesNoteFilterBy" ng-disabled="ng_readOnly">
                                </select>
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup6">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Filter Value</span>
                                <input id="SalesNoteFilterValue" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Type to search" ng-model="SalesNoteFilterValue" />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                                <input type="submit" value="Search" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn_Modal" ng-click="SalesNoteSearch();" />
                            </div>
                        </div>
                    </form>

                    <MyButton operation="@MyButtonOperation.ModalItemSelection" ng-click="SalesNoteSelectedAc({'ID': 0});" data-bs-dismiss="modal">Click to de select</MyButton>
                    <div ng-repeat="item in SalesNoteSearchResult" style="overflow:auto">
                        <MyButton operation="@MyButtonOperation.ModalItemSelection" ng-click="SalesNoteSelectedAc(item);" data-bs-dismiss="modal"><span style="color:steelblue;">[No#: {{item.InvoiceNo}}] [Date: {{item.InvoiceDate}}] [Qty: {{item.Quantity}}{{item.MeasurementUnit}}]</span></MyButton>
                    </div>
                </div>
                <div class="modal-footer">

                </div>
            </div>
        </div>
    </div>

</div>
