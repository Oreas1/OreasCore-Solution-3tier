<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'PurchaseReturnNoteDetailCtlr','PurchaseReturnNoteMasterCtlr', this);">Purchase Return Note</a>
    <MyViewName child="true"> Detail</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Ware House
                    </th>
                    <th>
                        Supplier A/c
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_WareHouseMaster_IDName}}
                    </td>
                    <td>
                        <b>{{MasterObject.FK_tbl_Ac_ChartOfAccounts_IDName}}</b>
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Inv_PurchaseReturnNoteDetail.ID", "{{tbl_Inv_PurchaseReturnNoteDetail.ID}}")
            @Html.Hidden("tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseReturnNoteMaster_ID", "{{tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseReturnNoteMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Product In {{tbl_Inv_PurchaseReturnNoteDetail.MeasurementUnit}}</span>
                        <MyButtonModal fieldname="tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" ng-click="OpenProductSearchModalGeneral('PurchaseReturnNote', tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseReturnNoteMaster_ID, 'tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName', ProductSearch_CtrlFunction_Ref_InvokeOnSelection, null, tbl_Inv_PurchaseReturnNoteDetail.ID); myform['tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName'].$dirty=true;" ng-disabled="ng_readOnly" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Reference #</span>
                        <MyButtonModal fieldname="tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_ID_ReferenceNoName" ng-click="OpenReferenceSearchModalGeneral('PurchaseReturnNote', tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseReturnNoteMaster_ID, 'tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_ID_ReferenceNoName', ReferenceSearch_CtrlFunction_Ref_InvokeOnSelection, null, tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_ID); myform['tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_ID_ReferenceNoName'].$dirty=true;" ng-disabled="ng_readOnly || !tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_ID > 0" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_ID_ReferenceNoName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Purchase #</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_IDName}}" ng-model="tbl_Inv_PurchaseReturnNoteDetail.FK_tbl_Inv_PurchaseNoteDetail_IDName" disabled />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Quantity [Max: {{Balance}}]</span>
                        <input id="tbl_Inv_PurchaseReturnNoteDetail.Quantity" name="tbl_Inv_PurchaseReturnNoteDetail.Quantity" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_PurchaseReturnNoteDetail.Quantity" ng-pattern="wholeNumberOrNot" ng-disabled="ng_readOnly" min="0" max="{{Balance}}" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_PurchaseReturnNoteDetail.Quantity" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input type="text" id="tbl_Inv_PurchaseReturnNoteDetail.Remarks" name="tbl_Inv_PurchaseReturnNoteDetail.Remarks" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Remarks" value="tbl_Inv_PurchaseReturnNoteDetail.Remarks" ng-model="tbl_Inv_PurchaseReturnNoteDetail.Remarks" ng-disabled="ng_readOnly" maxlength="50" />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_PurchaseReturnNoteDetail.Remarks" maxlength="50"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_PurchaseReturnNoteDetail.CreatedBy}}" ng-model="tbl_Inv_PurchaseReturnNoteDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_PurchaseReturnNoteDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_PurchaseReturnNoteDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_PurchaseReturnNoteDetail.ModifiedBy}}" ng-model="tbl_Inv_PurchaseReturnNoteDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_PurchaseReturnNoteDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_PurchaseReturnNoteDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Product Name
                </th>
                <th>
                    Reference #
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Purchase #
                </th>
                <th>
                    Remarks
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_PurchaseNoteDetail_ID_ReferenceNoName}}
                </td>
                <td>
                    {{itm.Quantity}} {{itm.MeasurementUnit}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_PurchaseNoteDetail_IDName}}
                </td>
                <td>
                    {{itm.Remarks}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="6">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "PurchaseReturnNoteDetailCtlr")
    @await Html.PartialAsync("_ReportPanelGeneral")

</div>
