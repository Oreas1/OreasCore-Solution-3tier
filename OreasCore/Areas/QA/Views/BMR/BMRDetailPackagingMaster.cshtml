<MyViewName>    
    <a href="" ng-click="DivHideShow(null,null,'BMRDetailPackagingMasterCtlr','BMRMasterCtlr', this);">Batch Record</a>
    <MyViewName child="true"> BPR Master</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Batch No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Mfg
                    </th>
                    <th>
                        Expiry
                    </th>
                    <th>
                        Product
                    </th>
                    <th>
                        Batch Size
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.BatchNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.BatchMfgDate}}
                    </td>
                    <td>
                        {{MasterObject.BatchExpiryDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                    </td>
                    <td>
                        {{MasterObject.BatchSize}}-{{MasterObject.BatchSizeUnit}}
                    </td>
                </tr>
                <tr>
                    <td colspan="6">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ID", "{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ID}}")
            @Html.Hidden("tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Pro_BatchMaterialRequisitionMaster_ID", "{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Pro_BatchMaterialRequisitionMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Primary Finished Product</span>
                        <MyButtonModal fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName" ng-click="OpenProductSearchModalGeneral('BMRPackaging', MasterObject.FK_tbl_Pro_CompositionDetail_Coupling_ID, 'tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName', ProductSearch_CtrlFunction_Ref_InvokeOnSelection, null, 0); myform['tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName'].$dirty=true;" ng-disabled="ng_readOnly || ng_readOnlyOnEdit" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3" ng-hide="ng_entryPanelSubmitBtnText==='Save New'">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Secondary Finished Product</span>
                        <MyButtonModal fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_SecondaryName" ng-click="OpenProductSearchModalGeneral('Child', tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_Primary, 'tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_SecondaryName', ProductSearch_CtrlFunction_Ref_InvokeOnSelection, null, 0); myform['tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_SecondaryName'].$dirty=true;" ng-disabled="ng_readOnly || tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.SecondaryUsedINPT"></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.FK_tbl_Inv_ProductRegistrationDetail_ID_SecondaryName"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">BatchSize In {{MasterObject.BatchSizeUnit}}</span>
                        <input id="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.BatchSize" name="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.BatchSize" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.BatchSize" ng-pattern="wholeNumberOrNot" ng-disabled="ng_readOnly" min="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.IsPackagingDispened ? tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.BatchSize : 1" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.BatchSize" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Packaging Name</span>
                        <input type="text" id="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.PackagingName" name="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.PackagingName" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="...." value="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.PackagingName" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.PackagingName" ng-disabled="ng_readOnly" minlength="1" maxlength="50" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.PackagingName" required="true" minlength="1" maxlength="50"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup1">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Dispensed</span>
                        <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.IsPackagingDispened ? 'Yes' : 'No'}}" disabled />
                    </div>
                </div>


                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>
                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.CreatedBy}}" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ModifiedBy}}" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_BatchMaterialRequisitionDetail_PackagingMaster.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>

            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Packaging Name
                </th>
                <th>
                    Batch Size
                </th>
                <th>
                    Primary Finished
                </th>
                <td>
                    GTIN #
                </td>
                <th>
                    Total Prod Primary
                </th>
                <th>
                    Secondary Finished
                </th>
                <th>
                    Total Prod Secondary
                </th>
                <th>
                    Order Detail
                </th>
                <td>
                    BPR Process(s)
                </td>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.PackagingName}}
                </td>
                <td>
                    {{itm.BatchSize}}-{{itm.BatchSizeUnit}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName}}
                </td>
                <td>
                    {{itm.GTINCode}}
                </td>
                <td>
                    {{itm.TotalProd_Primary}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_ID_SecondaryName}}
                </td>
                <td>
                    {{itm.TotalProd_Secondary}}
                </td>
                <td>
                    {{itm.OrderDetail}}
                </td>                
                <td>
                    {{itm.BPRProcesses}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="
                                itm['MasterObject']=MasterObject; 
                                DivHideShow('BMRDetailPackagingDetailFilterCtlr',itm,'BMRDetailPackagingMasterCtlr','BMRDetailPackagingDetailFilterCtlr', null);
                                DivHideShow('BPRProcessCtlr',itm,null,'BPRProcessCtlr', null);                                
                                "
                                mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Packaging Formula Type">Formula Type <span class="fa fa-cubes"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="10">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "BMRDetailPackagingMasterCtlr")
    @await Html.PartialAsync("_ReportPanelGeneral")

</div>
