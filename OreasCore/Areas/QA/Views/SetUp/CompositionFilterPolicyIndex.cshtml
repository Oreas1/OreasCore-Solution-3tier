@{
    ViewBag.Title = "QA CompositionFilterPolicy";
}
@section Scripts {
    <script src="~/angular.js/Angular-QA/SetUp/compositionfilterpolicyqaindex.0.1.js"></script>
}
<div ng-controller="CompositionFilterPolicyMasterCtlr" class="container">

    <div id="CompositionFilterPolicyMasterCtlr">
        <MyViewName>Composition Filter Policy</MyViewName>
        @*-----------------------------------this is entry panel-----------------------------*@
        <MyButton operation="@MyButtonOperation.CancelOfAdd"></MyButton>
        <div ng-hide="ng_entryPanelHide">
            <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate>
                <MyAntiforgeryToken></MyAntiforgeryToken>
                @Html.Hidden("tbl_Pro_CompositionFilterPolicyMaster.ID", "{{tbl_Pro_CompositionFilterPolicyMaster.ID}}")
                <MyFieldSet legendtext="Entry Form">

                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Product Type For Coupling</span>
                            <select id="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_For_Coupling" name="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_For_Coupling" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.ProductType for x in ProductType" data-ng-model="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_For_Coupling" ng-disabled="ng_readOnly" required>
                                <option value="">Select Product Type</option>
                            </select>
                            <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_For_Coupling" required="true"></MyValidationSpan>
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">WareHouse By</span>
                            <MyButtonModal fieldname="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_WareHouseMaster_ID_ByName" ng-click="OpenWHMSearchModalGeneral('General', 0, 'tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_WareHouseMaster_ID_ByName', WHMSearch_CtrlFunction_Ref_InvokeOnSelection);myform['tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_WareHouseMaster_ID_ByName'].$dirty=true;" ng-disabled="ng_readOnly" required></MyButtonModal>
                            <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_WareHouseMaster_ID_ByName" required="true"></MyValidationSpan>
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Product Type For QC Sample</span>
                            <select id="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_QCSample" name="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_QCSample" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.ProductType for x in ProductType" data-ng-model="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_QCSample" ng-disabled="ng_readOnly" required>
                                <option value="">Select Product Type</option>
                            </select>
                            <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Inv_ProductType_ID_QCSample" required="true"></MyValidationSpan>
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Default Action For QC Sample</span>
                            <select id="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Qc_ActionType_ID_BMRProcessTestingSample" name="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Qc_ActionType_ID_BMRProcessTestingSample" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.ActionName for x in ActionList" data-ng-model="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Qc_ActionType_ID_BMRProcessTestingSample" ng-disabled="ng_readOnly" required>
                                <option value="">Select Action</option>
                            </select>
                            <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionFilterPolicyMaster.FK_tbl_Qc_ActionType_ID_BMRProcessTestingSample" required="true"></MyValidationSpan>
                        </div>
                    </div>

                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                            <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                        </div>
                    </div>

                    <MyFieldSet legendtext="User" child="true">
                        <div mycssclass="@MyCSSClassName.RowInForm">
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionFilterPolicyMaster.CreatedBy}}" ng-model="tbl_Pro_CompositionFilterPolicyMaster.CreatedBy" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionFilterPolicyMaster.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_CompositionFilterPolicyMaster.CreatedDate" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionFilterPolicyMaster.ModifiedBy}}" ng-model="tbl_Pro_CompositionFilterPolicyMaster.ModifiedBy" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionFilterPolicyMaster.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_CompositionFilterPolicyMaster.ModifiedDate" disabled />
                            </div>
                        </div>
                    </MyFieldSet>
                </MyFieldSet>
            </form>
        </div>
        @*-----------------------------------this is data list view-----------------------------*@
        <div mycssclass="@MyCSSClassName.Table_Div">
            <table mycssclass="@MyCSSClassName.Table_Table">
                <tr>
                    <th>
                        Product Type For Coupling
                    </th>
                    <th>
                        WareHouse Requested By
                    </th>
                    <th>
                        Product Type For QC Sample
                    </th>
                    <th>
                        Default Action For QC Sample
                    </th>
                    <th></th>
                </tr>
                <tr ng-repeat="itm in pageddata.Data">
                    <td>
                        {{itm.FK_tbl_Inv_ProductType_ID_For_CouplingName}}
                    </td>
                    <td>
                        {{itm.FK_tbl_Inv_WareHouseMaster_ID_ByName}}
                    </td>
                    <td>
                        {{itm.FK_tbl_Inv_ProductType_ID_QCSampleName}}
                    </td>
                    <td>
                        {{itm.FK_tbl_Qc_ActionType_ID_BMRProcessTestingSampleName}}
                    </td>
                    <td>
                        <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                            <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                            <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>
                        </div>
                        <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                            <button ng-click="DivHideShow('CompositionFilterPolicyDetailCtlr',itm,'CompositionFilterPolicyMasterCtlr','CompositionFilterPolicyDetailCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Detail">Detail <span class="fa fa-info-circle"></span></button>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="5">
                        <div mycssclass="@MyCSSClassName.Table_NavDiv">
                            <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                            <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                            <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                            <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                            <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                        </div>
                    </td>
                </tr>
            </table>
        </div>

        @*-----------------------------------this is search view-----------------------------*@

    </div>

    <div ng-controller="CompositionFilterPolicyDetailCtlr">
        <div id="CompositionFilterPolicyDetailCtlr" style="display: none;">
            @await Html.PartialAsync("CompositionFilterPolicyDetail")
        </div>
    </div>

    @*-----------------------------------this is Modal view-----------------------------*@
    @await Html.PartialAsync("_WHMSearchModalGeneral")
</div>




