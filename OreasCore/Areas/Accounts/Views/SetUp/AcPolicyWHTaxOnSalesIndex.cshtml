@{
    ViewBag.Title = "PolicyWHTaxOnSales";
}
@section Scripts {
    <script src="~/angular.js/Angular-Accounts/SetUp/acpolicywhtaxonsalesindex.0.1.js"></script>
}
<div ng-controller="AcPolicyWHTaxOnSalesMasterCtlr" class="container">

    <div id="AcPolicyWHTaxOnSalesMasterCtlr">
        <MyViewName>Policy WHTax On Sales</MyViewName>
      @*-----------------------------------this is entry panel-----------------------------*@
        <MyButton operation="@MyButtonOperation.Add"></MyButton>
        <div ng-hide="ng_entryPanelHide">
            <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate>
                <MyAntiforgeryToken></MyAntiforgeryToken>
                @Html.Hidden("tbl_Ac_PolicyWHTaxOnSales.ID", "{{tbl_Ac_PolicyWHTaxOnSales.ID}}")
                <MyFieldSet legendtext="Entry Form">

                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Policy Name</span>
                            <input id="tbl_Ac_PolicyWHTaxOnSales.WHTaxName" name="tbl_Ac_PolicyWHTaxOnSales.WHTaxName" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="...." value="tbl_Ac_PolicyWHTaxOnSales.WHTaxName" ng-model="tbl_Ac_PolicyWHTaxOnSales.WHTaxName" ng-disabled="ng_readOnly" minlength="3" maxlength="50" required />
                            <MyValidationSpan formname="myform" fieldname="tbl_Ac_PolicyWHTaxOnSales.WHTaxName" required="true" minlength="3" maxlength="50"></MyValidationSpan>
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">WHTax %</span>
                            <input id="tbl_Ac_PolicyWHTaxOnSales.WHTaxPer" name="tbl_Ac_PolicyWHTaxOnSales.WHTaxPer" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Ac_PolicyWHTaxOnSales.WHTaxPer" ng-disabled="ng_readOnly" min="0" max="100" required />
                            <MyValidationSpan formname="myform" fieldname="tbl_Ac_PolicyWHTaxOnSales.WHTaxPer" required="true"></MyValidationSpan>
                        </div>
                    </div>

                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                            <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                        </div>
                    </div>

                    <MyFieldSet legendtext="User" child="true">
                        <div mycssclass="@MyCSSClassName.RowInForm">
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_PolicyWHTaxOnSales.CreatedBy}}" ng-model="tbl_Ac_PolicyWHTaxOnSales.CreatedBy" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_PolicyWHTaxOnSales.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_PolicyWHTaxOnSales.CreatedDate" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_PolicyWHTaxOnSales.ModifiedBy}}" ng-model="tbl_Ac_PolicyWHTaxOnSales.ModifiedBy" disabled />
                            </div>
                            <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                                <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                                <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_PolicyWHTaxOnSales.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_PolicyWHTaxOnSales.ModifiedDate" disabled />
                            </div>
                        </div>
                    </MyFieldSet>
                </MyFieldSet>
            </form>
        </div>
        @*-----------------------------------this is data list view-----------------------------*@
        <div mycssclass="@MyCSSClassName.Table_Div">
            <table mycssclass="@MyCSSClassName.Table_Table">
                <tr>
                    <th>
                        Policy Name
                    </th>
                    <th>
                        WHTax %
                    </th>
                    <th>
                        Total Account
                    </th>
                    <th></th>
                </tr>
                <tr ng-repeat="itm in pageddata.Data">
                    <td>
                        {{itm.WHTaxName}}
                    </td>
                    <td>
                        {{itm.WHTaxPer}}
                    </td>
                    <td>
                        {{itm.TotalAc}}
                    </td>
                    <td>
                        <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                            <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                            <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                            <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                        </div>
                        <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                            <button ng-click="DivHideShow('AcPolicyWHTaxOnSalesDetailCtlr',itm,'AcPolicyWHTaxOnSalesMasterCtlr','AcPolicyWHTaxOnSalesDetailCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Linked Accounts">Linked Accounts <span class="fa fa-joint"></span></button>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                        <div mycssclass="@MyCSSClassName.Table_NavDiv">
                            <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                            <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                            <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                            <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                            <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                        </div>
                    </td>
                </tr>
            </table>
        </div>

        @*-----------------------------------this is search view-----------------------------*@
        @await Html.PartialAsync("_Filter", "AcPolicyWHTaxOnSalesMasterCtlr")
    </div>


    <div ng-controller="AcPolicyWHTaxOnSalesDetailCtlr">
        <div id="AcPolicyWHTaxOnSalesDetailCtlr" style="display: none;">
            @await Html.PartialAsync("AcPolicyWHTaxOnSalesDetail")
        </div>
    </div>

        
</div>




