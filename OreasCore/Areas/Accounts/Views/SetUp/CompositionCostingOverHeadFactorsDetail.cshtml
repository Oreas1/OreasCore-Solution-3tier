<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'CompositionCostingOverHeadFactorsDetailCtlr','CompositionCostingOverHeadFactorsMasterCtlr', this);">Costing OverHead Factors</a>
    <MyViewName child="true"> Formula(s)</MyViewName>
</MyViewName>

<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Group Name
                    </th>
                    <th>
                        Default
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.GroupName}}
                    </td>
                    <td>
                        {{MasterObject.IsDefault ? 'Yes' : 'No' }}
                    </td>
                </tr>
                <tr>
                    <td colspan="2">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate>

            <MyAntiforgeryToken></MyAntiforgeryToken>
            @Html.Hidden("tbl_Ac_CompositionCostingFactorsDetail.ID", "{{tbl_Ac_CompositionCostingFactorsDetail.ID}}")
            @Html.Hidden("tbl_Ac_CompositionCostingFactorsDetail.FK_tbl_Ac_CompositionCostingOverHeadFactorsMaster_ID", "{{tbl_Ac_CompositionCostingFactorsDetail.FK_tbl_Ac_CompositionCostingOverHeadFactorsMaster_ID}}")
            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">FormulaName</span>
                        <input type="text" id="tbl_Ac_CompositionCostingFactorsDetail.FormulaName" name="tbl_Ac_CompositionCostingFactorsDetail.FormulaName" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="FormulaName" value="tbl_Ac_CompositionCostingFactorsDetail.FormulaName" ng-model="tbl_Ac_CompositionCostingFactorsDetail.FormulaName" ng-disabled="ng_readOnly" minlength="3" maxlength="50" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_CompositionCostingFactorsDetail.FormulaName" required="true" minlength="3" maxlength="50"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup8">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Expression Allowed + - * / () C</span>
                        <input type="text" id="tbl_Ac_CompositionCostingFactorsDetail.FormulaExpression" name="tbl_Ac_CompositionCostingFactorsDetail.FormulaExpression" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="(C*0.05)+C" value="tbl_Ac_CompositionCostingFactorsDetail.FormulaExpression" ng-model="tbl_Ac_CompositionCostingFactorsDetail.FormulaExpression" ng-disabled="ng_readOnly" minlength="2" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_CompositionCostingFactorsDetail.FormulaExpression" required="true" minlength="2"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_CompositionCostingFactorsDetail.CreatedBy}}" ng-model="tbl_Ac_CompositionCostingFactorsDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_CompositionCostingFactorsDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_CompositionCostingFactorsDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_CompositionCostingFactorsDetail.ModifiedBy}}" ng-model="tbl_Ac_CompositionCostingFactorsDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_CompositionCostingFactorsDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_CompositionCostingFactorsDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Formula Name
                </th>
                <th>
                    Formula Expression
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FormulaName}}
                </td>
                <td>
                    {{itm.FormulaExpression}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="3">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>
        </table>
    </div>


    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "CompositionCostingOverHeadFactorsDetailCtlr")

</div>
