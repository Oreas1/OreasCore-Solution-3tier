<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'SalesNoteDetailCtlr','SalesNoteMasterCtlr', this);">Sales Note</a>
    <MyViewName child="true"> Detail</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Ware House
                    </th>
                    <th>
                        Customer A/c
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_WareHouseMaster_IDName}}
                    </td>
                    <td>
                        <b>{{MasterObject.FK_tbl_Ac_ChartOfAccounts_IDName}}</b>
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.CancelOfAdd"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow('Load'); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Inv_SalesNoteDetail.ID", "{{tbl_Inv_SalesNoteDetail.ID}}")
            @Html.Hidden("tbl_Inv_SalesNoteDetail.FK_tbl_Inv_SalesNoteMaster_ID", "{{tbl_Inv_SalesNoteDetail.FK_tbl_Inv_SalesNoteMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Product In {{tbl_Inv_SalesNoteDetail.MeasurementUnit}}</span>
                        <input id="tbl_Inv_SalesNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" name="tbl_Inv_SalesNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_Inv_SalesNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName}}" ng-model="tbl_Inv_SalesNoteDetail.FK_tbl_Inv_ProductRegistrationDetail_IDName" ng-disabled="ng_readOnly" readonly />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Quantity</span>
                        <input id="tbl_Inv_SalesNoteDetail.Quantity" name="tbl_Inv_SalesNoteDetail.Quantity" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.Quantity" ng-disabled="ng_readOnly" readonly />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Reference#</span>
                        <input id="tbl_Inv_SalesNoteDetail.ReferenceNo" name="tbl_Inv_SalesNoteDetail.ReferenceNo" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Auto" value="tbl_Inv_SalesNoteDetail.ReferenceNo" ng-model="tbl_Inv_SalesNoteDetail.ReferenceNo" ng-disabled="ng_readOnly" readonly />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label"># Packages</span>
                        <input id="tbl_Inv_SalesNoteDetail.NoOfPackages" name="tbl_Inv_SalesNoteDetail.NoOfPackages" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.NoOfPackages" ng-disabled="ng_readOnly" readonly />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Remarks</span>
                        <input type="text" id="tbl_Inv_SalesNoteDetail.Remarks" name="tbl_Inv_SalesNoteDetail.Remarks" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="Remarks" value="tbl_Inv_SalesNoteDetail.Remarks" ng-model="tbl_Inv_SalesNoteDetail.Remarks" ng-disabled="ng_readOnly" readonly />
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">
                            <span>Rate</span>&nbsp;
                            <span style="font-size:xx-small; width:100%; text-align:right; color:darkred;">[ON Rate: {{tbl_Inv_SalesNoteDetail.ONRate}}]</span>
                        </div>
                        <input id="tbl_Inv_SalesNoteDetail.Rate" name="tbl_Inv_SalesNoteDetail.Rate" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.Rate" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesNoteDetail.Rate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">ST %</span>
                        <input id="tbl_Inv_SalesNoteDetail.STPercentage" name="tbl_Inv_SalesNoteDetail.STPercentage" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.STPercentage" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesNoteDetail.STPercentage" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">FST %</span>
                        <input id="tbl_Inv_SalesNoteDetail.FSTPercentage" name="tbl_Inv_SalesNoteDetail.FSTPercentage" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.FSTPercentage" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesNoteDetail.FSTPercentage" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">WHT %&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <input type="button" class="btn btn-sm btn-outline-primary" value="Apply WHT {{tbl_Inv_SalesNoteDetail.DefaultWHTPer}}%" ng-click="tbl_Inv_SalesNoteDetail.WHTPercentage=tbl_Inv_SalesNoteDetail.DefaultWHTPer;" /></span>
                        <input id="tbl_Inv_SalesNoteDetail.WHTPercentage" name="tbl_Inv_SalesNoteDetail.WHTPercentage" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.WHTPercentage" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesNoteDetail.WHTPercentage" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Discount</span>
                        <input id="tbl_Inv_SalesNoteDetail.DiscountAmount" name="tbl_Inv_SalesNoteDetail.DiscountAmount" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Inv_SalesNoteDetail.DiscountAmount" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Inv_SalesNoteDetail.DiscountAmount" required="true"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesNoteDetail.CreatedBy}}" ng-model="tbl_Inv_SalesNoteDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesNoteDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_SalesNoteDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesNoteDetail.ModifiedBy}}" ng-model="tbl_Inv_SalesNoteDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Inv_SalesNoteDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Inv_SalesNoteDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Product Name
                </th>
                <th>
                    Ref #
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Rate
                </th>
                <th>
                    Gross
                </th>
                <th>
                    ST
                </th>
                <th>
                    FST
                </th>
                <th>
                    WHT
                </th>
                <th>
                    Discount
                </th>
                <th>
                    Net Amount
                </th>
                <th>
                    Status
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_IDName}}
                </td>
                <td>
                    {{itm.ReferenceNo}}
                </td>
                <td>
                    {{itm.Quantity}} {{itm.MeasurementUnit}}
                </td>
                <td>
                    {{itm.Rate}}
                </td>
                <td>
                    {{itm.GrossAmount}}
                </td>
                <td>
                    {{itm.STAmount}}<br />[{{itm.STPercentage}}%]
                </td>
                <td>
                    {{itm.FSTAmount}}<br />[{{itm.FSTPercentage}}%]
                </td>
                <td>
                    {{itm.WHTAmount}}<br />[{{itm.WHTPercentage}}%]
                </td>
                <td>
                    {{itm.DiscountAmount}}
                </td>
                <td>
                    {{itm.NetAmount}}
                </td>
                <td>
                    <i class="fa fa-lightbulb" style="color: green; font-size: large;" ng-hide="itm.IsProcessed===false"></i>
                    <i class="fa fa-lightbulb" style="color: red; font-size: large;" ng-hide="itm.IsProcessed===true"></i>
                    Processed
                    <br />
                    <i class="fa fa-lightbulb" style="color: green; font-size: large;" ng-hide="itm.IsSupervised===false"></i>
                    <i class="fa fa-lightbulb" style="color: red; font-size: large;" ng-hide="itm.IsSupervised===true"></i>
                    Supervised
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.AddDetail" ng-click="GetRow(itm.ID,'Add')" ng-hide="itm.IsProcessed"></MyButton>
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')" ng-hide="!itm.IsProcessed"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('SalesNoteDetailReturnCtlr',itm,'SalesNoteDetailCtlr','SalesNoteDetailReturnCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Return Detail">Return</button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="12">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "SalesNoteDetailCtlr")
    @await Html.PartialAsync("_ReportPanelGeneral")
</div>
