<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'JournalDocumentDetailCtlr','JournalDocumentMasterCtlr', this);">Journal Document</a>
    <MyViewName child="true"> Detail</MyViewName>
</MyViewName>

<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Voucher No
                    </th>
                    <th>
                        Voucher Date
                    </th>
                    <th>
                        From Account
                    </th>
                    <th>
                        Debit/Credit
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.VoucherNo}}
                    </td>
                    <td>
                        {{MasterObject.VoucherDate}}
                    </td>
                    <td style="background-color:lightsteelblue;">
                        <b>{{MasterObject.FK_tbl_Ac_ChartOfAccounts_IDName}}</b>
                    </td>
                    <td>
                        {{MasterObject.Debit1_Credit0 ? 'Debit' : 'Credit'}}
                    </td>
                </tr>
                <tr>
                    <td colspan="4">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Ac_V_JournalDocumentDetail.ID", "{{tbl_Ac_V_JournalDocumentDetail.ID}}")
            @Html.Hidden("tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_V_JournalDocumentMaster_ID", "{{tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_V_JournalDocumentMaster_ID}}")
            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Account To</span>
                        <MyButtonModal fieldname="tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_IDName" ng-click="OpenCOASearchModalGeneral('JournalDocDetail', 0, 'tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_IDName', COASearch_CtrlFunction_Ref_InvokeOnSelection);myform['tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_IDName'].$dirty=true;" ng-disabled="ng_readOnly" required></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_IDName" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Posting Date</span>
                        <input id="tbl_Ac_V_JournalDocumentDetail.PostingDate" name="tbl_Ac_V_JournalDocumentDetail.PostingDate" type="datetime-local" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_Ac_V_JournalDocumentDetail.PostingDate}}" ng-model="tbl_Ac_V_JournalDocumentDetail.PostingDate" ng-disabled="ng_readOnly" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_V_JournalDocumentDetail.PostingDate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Amount</span>
                        <input id="tbl_Ac_V_JournalDocumentDetail.Amount" name="tbl_Ac_V_JournalDocumentDetail.Amount" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_Ac_V_JournalDocumentDetail.Amount" ng-disabled="ng_readOnly" min="0" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_V_JournalDocumentDetail.Amount" required="true"></MyValidationSpan>
                    </div>
                </div>
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Narration</span>
                        <input id="tbl_Ac_V_JournalDocumentDetail.Narration" name="tbl_Ac_V_JournalDocumentDetail.Narration" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="...." value="tbl_Ac_V_JournalDocumentDetail.Narration" ng-model="tbl_Ac_V_JournalDocumentDetail.Narration" ng-disabled="ng_readOnly" minlength="1" maxlength="100" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_V_JournalDocumentDetail.Narration" required="true" minlength="1" maxlength="100"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Account For</span>
                        <MyButtonModal fieldname="tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_ID_ForName" ng-click="OpenCOASearchModalGeneral('JournalDocDetail', 0, 'tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_ID_ForName', COASearch_CtrlFunction_Ref_InvokeOnSelection);myform['tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_ID_ForName'].$dirty=true;" ng-disabled="ng_readOnly"></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Ac_V_JournalDocumentDetail.FK_tbl_Ac_ChartOfAccounts_ID_ForName"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_V_JournalDocumentDetail.CreatedBy}}" ng-model="tbl_Ac_V_JournalDocumentDetail.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_V_JournalDocumentDetail.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_V_JournalDocumentDetail.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_V_JournalDocumentDetail.ModifiedBy}}" ng-model="tbl_Ac_V_JournalDocumentDetail.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Ac_V_JournalDocumentDetail.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Ac_V_JournalDocumentDetail.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Account Name
                </th>
                <th>
                    Posting Date
                </th>
                <th>
                    Debit/Credit
                </th>
                <th>
                    Narration
                </th>
                <th>
                    Amount
                </th>
                <th>
                    Ac For
                </th>
                <th>
                    Supervised
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Ac_ChartOfAccounts_IDName}}
                </td>
                <td>
                    {{itm.PostingDate}}
                </td>
                <td>
                    {{MasterObject.Debit1_Credit0 ? 'Credit' : 'Debit'}}
                </td>
                <td>
                    {{itm.Narration}}
                </td>
                <td>
                    {{itm.Amount}}
                </td>
                <td>
                    {{itm.FK_tbl_Ac_ChartOfAccounts_ID_ForName}}
                </td>
                <td>
                    <i class="fa fa-lightbulb" style="color: green; font-size: large;" ng-hide="itm.IsSupervised===false"></i>
                    <i class="fa fa-lightbulb" style="color: red; font-size: large;" ng-hide="itm.IsSupervised===true"></i>
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="8">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "JournalDocumentDetailCtlr")
    @await Html.PartialAsync("_ReportPanelGeneral")

    @*-----------------------------------Excell-----------------------------*@
    <div>
        <table class="table">
            <tr>
                <td>
                    <input type="file" id="UploadExcelFile" name="UploadExcelFile" ng-disabled="!Privilege.CanAdd" onchange="angular.element(this).scope().LoadFileData(this.files)" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" class="btn btn-block" />
                </td>
                <td>
                    Excel Format
                </td>
                <td>

                    <table border="1">
                        <tr style="border:1px solid black;">
                            <th style="border: 1px solid black; background-color: lightpink;">&nbsp;&nbsp; *AcCode &nbsp;&nbsp;</th>
                            <th style="border: 1px solid black; background-color: lightpink;">&nbsp; *Narration &nbsp;</th>
                            <th style="border: 1px solid black; background-color: lightpink;">&nbsp;&nbsp; *Amount &nbsp;&nbsp;</th>
                            <th style="border: 1px solid black; background-color: lightpink;">&nbsp;&nbsp; *Posting Date &nbsp;&nbsp;</th>
                        </tr>
                        <tr>
                            <td style="border:1px solid black;">3-1-2-5</td>
                            <td style="border: 1px solid black; text-align: right;">Payment Against Supplies</td>
                            <td style="border: 1px solid black; text-align: right;">25000</td>
                            <td style="border: 1px solid black; text-align: center;">29-12-2020 14:30:01 <br /> [dd-MM-yyyy HH:mm:ss]</td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>



    </div>
</div>
