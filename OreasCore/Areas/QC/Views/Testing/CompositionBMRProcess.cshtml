<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'CompositionBMRProcessCtlr','CompositionMasterCtlr', this);">QC Composition Testing</a>
    <MyViewName child="true"> BMR Process</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Composition
                    </th>
                    <th>
                        Dimension
                    </th>
                    <th>
                        Shelf Life
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.CompositionName}}
                    </td>
                    <td>
                        {{MasterObject.DimensionValue}} {{MasterObject.FK_tbl_Inv_MeasurementUnit_ID_DimensionName}}
                    </td>
                    <td>
                        {{MasterObject.ShelfLifeInMonths}} Month(s)
                    </td>
                </tr>
                <tr>
                    <td colspan="5">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@
    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_Pro_CompositionMaster_ProcessBMR.ID", "{{tbl_Pro_CompositionMaster_ProcessBMR.ID}}")
            @Html.Hidden("tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_CompositionMaster_ID", "{{tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_CompositionMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Procedure</span>
                        <select id="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_Procedure_ID" name="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_Procedure_ID" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="x.ID as x.ProcedureName for x in BMRProcedureList" data-ng-model="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_Procedure_ID" ng-disabled="ng_readOnly" required>
                            <option value="">Select Procedure</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Pro_Procedure_ID" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup4">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Sample Product For QC</span>
                        <MyButtonModal fieldname="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Inv_ProductRegistrationDetail_ID_QCSampleName" ng-click="OpenProductSearchModalGeneral('ProProcess', 0, 'tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Inv_ProductRegistrationDetail_ID_QCSampleName', ProductSearch_CtrlFunction_Ref_InvokeOnSelection, null, 0); myform['tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Inv_ProductRegistrationDetail_ID_QCSampleName'].$dirty=true;" ng-disabled="ng_readOnly"></MyButtonModal>
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionMaster_ProcessBMR.FK_tbl_Inv_ProductRegistrationDetail_ID_QCSampleName"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req"> QA Clearance Before Start</span>
                        <select id="tbl_Pro_CompositionMaster_ProcessBMR.IsQAClearanceBeforeStart" name="tbl_Pro_CompositionMaster_ProcessBMR.IsQAClearanceBeforeStart" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="o.v as o.n for o in [{ n: 'Yes', v: true }, { n: 'No', v: false }]" data-ng-model="tbl_Pro_CompositionMaster_ProcessBMR.IsQAClearanceBeforeStart" ng-disabled="ng_readOnly" required>
                            <option value="">Select</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_Pro_CompositionMaster_ProcessBMR.IsQAClearanceBeforeStart" required="true"></MyValidationSpan>
                    </div>
                </div>

                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionMaster_ProcessBMR.CreatedBy}}" ng-model="tbl_Pro_CompositionMaster_ProcessBMR.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionMaster_ProcessBMR.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_CompositionMaster_ProcessBMR.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionMaster_ProcessBMR.ModifiedBy}}" ng-model="tbl_Pro_CompositionMaster_ProcessBMR.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_Pro_CompositionMaster_ProcessBMR.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_Pro_CompositionMaster_ProcessBMR.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>
    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Procedure Name
                </th>
                <th>
                    QC Sample Product
                </th>
                <th>
                    QA Req
                </th>
                <th>
                    Total Tests
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.FK_tbl_Pro_Procedure_IDName}}
                </td>
                <td>
                    {{itm.FK_tbl_Inv_ProductRegistrationDetail_ID_QCSampleName}}
                </td>
                <td>
                    {{itm.IsQAClearanceBeforeStart ? 'Yes': 'No'}}
                </td>
                <td>
                    {{itm.TotalTests}}
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('CompositionBMRProcessQcTestCtlr',itm,'CompositionBMRProcessCtlr','CompositionBMRProcessQcTestCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Testing">Testing <span class="fa fa-flask-vial"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="5">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "CompositionBMRProcessCtlr")

</div>
