<MyViewName>
    <a href="" ng-click="DivHideShow(null,null,'RequestDetailTRCtlr','RequestMasterCtlr', this);">Trial Request</a>
    <MyViewName child="true"> Trial BMR</MyViewName>
</MyViewName>
<div>
    <div>
        <div mycssclass="@MyCSSClassName.Table_DivSubPage">
            <table mycssclass="@MyCSSClassName.Table_TableSubPage">
                <tr>
                    <th>
                        Doc No
                    </th>
                    <th>
                        Doc Date
                    </th>
                    <th>
                        Semi Finish
                    </th>
                    <th>
                        Primary Unit
                    </th>
                    <th>
                        Shelf Life
                    </th>
                    <th>
                        MRP
                    </th>
                </tr>
                <tr>
                    <td>
                        {{MasterObject.DocNo}}
                    </td>
                    <td>
                        {{MasterObject.DocDate}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_ProductRegistrationDetail_IDName}} {{MasterObject.MeasurementUnit}}
                    </td>
                    <td>
                        {{MasterObject.FK_tbl_Inv_ProductRegistrationDetail_ID_PrimaryName}}
                    </td>
                    <td>
                        {{MasterObject.SampleProductExpiryMonths}} Month(s)
                    </td>
                    <td>
                        {{MasterObject.SampleProductMRP}}
                    </td>
                </tr>
                <tr>
                    <td colspan="6">
                    </td>
                </tr>
            </table>
        </div>
    </div>

    @*-----------------------------------this is entry panel-----------------------------*@

    <MyButton operation="@MyButtonOperation.Add"></MyButton>
    <div ng-hide="ng_entryPanelHide">
        <form name="myform" role="form" ng-submit="PostRow(); myform.$setPristine();" novalidate="">
            @Html.Hidden("tbl_PD_RequestDetailTR.ID", "{{tbl_PD_RequestDetailTR.ID}}")
            @Html.Hidden("tbl_PD_RequestDetailTR.FK_tbl_PD_RequestMaster_ID", "{{tbl_PD_RequestDetailTR.FK_tbl_PD_RequestMaster_ID}}")

            <MyFieldSet legendtext="Entry Form">
                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Doc No</span>
                        <input id="tbl_PD_RequestDetailTR.DocNo" name="tbl_PD_RequestDetailTR.DocNo" type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="00001" value="tbl_PD_RequestDetailTR.DocNo" ng-model="tbl_PD_RequestDetailTR.DocNo" ng-disabled="ng_readOnly" readonly />
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Doc Date</span>
                        <input id="tbl_PD_RequestDetailTR.DocDate" name="tbl_PD_RequestDetailTR.DocDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_PD_RequestDetailTR.DocDate}}" ng-model="tbl_PD_RequestDetailTR.DocDate" ng-disabled="ng_readOnly || (tbl_PD_RequestDetailTR.ID > 0 && ng_entryPanelSubmitBtnText==='Save New')" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_PD_RequestDetailTR.DocDate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Mfg Date</span>
                        <input id="tbl_PD_RequestDetailTR.MfgDate" name="tbl_PD_RequestDetailTR.MfgDate" type="date" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="{{tbl_PD_RequestDetailTR.MfgDate}}" ng-model="tbl_PD_RequestDetailTR.MfgDate" ng-disabled="ng_readOnly || (tbl_PD_RequestDetailTR.ID>0 && ng_entryPanelSubmitBtnText==='Save New')" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_PD_RequestDetailTR.MfgDate" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Trial Batch#</span>
                        <input type="text" id="tbl_PD_RequestDetailTR.TrialBatchNo" name="tbl_PD_RequestDetailTR.TrialBatchNo" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" placeholder="...." value="tbl_PD_RequestDetailTR.TrialBatchNo" ng-model="tbl_PD_RequestDetailTR.TrialBatchNo" ng-disabled="ng_readOnly || (tbl_PD_RequestDetailTR.ID>0 && ng_entryPanelSubmitBtnText==='Save New')" minlength="1" maxlength="25" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_PD_RequestDetailTR.TrialBatchNo" required="true" minlength="1" maxlength="25"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Trial BatchSize In {{MasterObject.MeasurementUnit}}</span>
                        <input id="tbl_PD_RequestDetailTR.TrialBatchSizeInSemiUnits" name="tbl_PD_RequestDetailTR.TrialBatchSizeInSemiUnits" type="number" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" ng-model="tbl_PD_RequestDetailTR.TrialBatchSizeInSemiUnits" ng-disabled="ng_readOnly || (tbl_PD_RequestDetailTR.ID>0 && ng_entryPanelSubmitBtnText==='Save New')" min="1" step="1" required />
                        <MyValidationSpan formname="myform" fieldname="tbl_PD_RequestDetailTR.TrialBatchSizeInSemiUnits" required="true"></MyValidationSpan>
                    </div>
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroup2">
                        <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label_Req">Status</span>
                        <select id="tbl_PD_RequestDetailTR.TrialStatus" name="tbl_PD_RequestDetailTR.TrialStatus" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Select" ng-options="o.v as o.n for o in [{ n: 'Sucessfull', v: true }, { n: 'Failed', v: false }, { n: 'Pending', v: null }]" data-ng-model="tbl_PD_RequestDetailTR.TrialStatus" ng-disabled="ng_readOnly">
                            <option value="">Select Completed</option>
                        </select>
                        <MyValidationSpan formname="myform" fieldname="tbl_PD_RequestDetailTR.TrialStatus"></MyValidationSpan>
                    </div>
                </div>



                <div mycssclass="@MyCSSClassName.RowInForm">
                    <div mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit">
                        <input type="submit" value="{{ng_entryPanelSubmitBtnText}}" mycssclass="@MyCSSClassName.RowInForm_FormGroupSubmit_btn" ng-disabled="myform.$invalid || ng_DisabledBtnAll" />
                    </div>
                </div>

                <MyFieldSet legendtext="User" child="true">
                    <div mycssclass="@MyCSSClassName.RowInForm">
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR.CreatedBy}}" ng-model="tbl_PD_RequestDetailTR.CreatedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Created Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR.CreatedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_PD_RequestDetailTR.CreatedDate" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified by</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR.ModifiedBy}}" ng-model="tbl_PD_RequestDetailTR.ModifiedBy" disabled />
                        </div>
                        <div mycssclass="@MyCSSClassName.RowInForm_FormGroup3">
                            <span mycssclass="@MyCSSClassName.RowInForm_FormGroup_Label">Modified Date</span>
                            <input type="text" mycssclass="@MyCSSClassName.RowInForm_FormGroup_Input" value="{{tbl_PD_RequestDetailTR.ModifiedDate | date :  'dd-MMM-yyyy HH:mm:ss a'}}" ng-model="tbl_PD_RequestDetailTR.ModifiedDate" disabled />
                        </div>
                    </div>
                </MyFieldSet>
            </MyFieldSet>
        </form>
    </div>

    @*-----------------------------------this is data list view-----------------------------*@
    <div mycssclass="@MyCSSClassName.Table_Div">
        <table mycssclass="@MyCSSClassName.Table_Table">
            <tr>
                <th>
                    Doc No
                </th>
                <th>
                    Doc Date
                </th>
                <th>
                    Mfg Date
                </th>
                <th>
                    Trial Batch#
                </th>
                <th>
                    Trial Batch Size
                </th>
                <th>
                    Dispensed
                </th>
                <th>
                    Status
                </th>
                <th></th>
            </tr>
            <tr ng-repeat="itm in pageddata.Data">
                <td>
                    {{itm.DocNo}}
                </td>
                <td>
                    {{itm.DocDate}}
                </td>
                <td>
                    {{itm.MfgDate}}
                </td>
                <td>
                    {{itm.TrialBatchNo}}
                </td>
                <td>
                    {{itm.TrialBatchSizeInSemiUnits}} {{MasterObject.MeasurementUnit}}
                </td>
                <td>
                    {{itm.IsDispensedAll === true ? 'Yes' : 'No'}}
                </td>
                <td>
                    <button class="btn" ng-style="{'background-color': itm.TrialStatus===true ? 'lightgreen' : itm.TrialStatus===false ? 'lightpink' : 'lightgray'}" ng-disabled="itm.TrialStatus != null" ng-click="GetRow(itm.ID,'Add')">
                        {{itm.TrialStatus===true ? 'Sucessfull' : itm.TrialStatus===false ? 'Failed' : 'Pending'}}
                    </button>   
                </td>
                <td>
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <MyButton operation="@MyButtonOperation.View" ng-click="GetRow(itm.ID,'View')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Edit" ng-click="GetRow(itm.ID,'Edit')"></MyButton>|
                        <MyButton operation="@MyButtonOperation.Delete" ng-click="GetRow(itm.ID,'Delete')"></MyButton>
                    </div>                    
                    <div mycssclass="@MyCSSClassName.Table_OptColDiv">
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('RequestDetailTRCFPCtlr',itm,'RequestDetailTRCtlr','RequestDetailTRCFPCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Composition">Comp<span class="fa fa-boxes"></span></button>|
                        <button ng-click="itm['MasterObject']=MasterObject; DivHideShow('RequestDetailTRProcedureCtlr',itm,'RequestDetailTRCtlr','RequestDetailTRProcedureCtlr', null);" mycssclass="@MyCSSClassName.Table_OperationBtn" title="Goto Procedures">Proce<span class="fa fa-gears"></span></button>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="10">
                    <div mycssclass="@MyCSSClassName.Table_NavDiv">
                        <MyButton operation="@MyButtonOperation.first" ng-click="pageNavigation('first')" ng-disabled="ng_DisabledBtnPageF"></MyButton>
                        <MyButton operation="@MyButtonOperation.back" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP"></MyButton>
                        <MyButton operation="@MyButtonOperation.RecordNo" ng-click="pageNavigation('back')" ng-disabled="ng_DisabledBtnPageP">{{pageddata.CurrentPage}} of {{pageddata.TotalPages}}</MyButton>
                        <MyButton operation="@MyButtonOperation.next" ng-click="pageNavigation('next')" ng-disabled="ng_DisabledBtnPageN"></MyButton>
                        <MyButton operation="@MyButtonOperation.last" ng-click="pageNavigation('last')" ng-disabled="ng_DisabledBtnPageL"></MyButton>
                    </div>
                </td>
            </tr>

        </table>
    </div>
    @*-----------------------------------this is search view-----------------------------*@
    @await Html.PartialAsync("_Filter", "RequestDetailTRCtlr")

</div>
